    "version": "0.2.0",
    "configurations": [
        {
            "name": "process_expansion",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${fileDirname}",
            "args": [
                "alpha_ketoglutarate_to_hopa_gen_2_tan_sample_0_n_samples_1000.pk",
                "2",
            ]
        },
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${fileDirname}",
            "args": [
                "../data/rules/minimal1224_all_uniprot.tsv",
                "../data/sprhea/sprhea_v3_20_random.json",
                "../artifacts/test_mapping.tsv"
            ],
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "${fileDirname}"
        }
    ]
}